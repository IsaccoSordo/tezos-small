{
  "name": "tezos-small",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run set-env && ng serve",
    "build": "npm run set-env && ng build",
    "build:prod": "npm run set-env && ng build --configuration production",
    "watch": "npm run set-env && ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,json}\"",
    "set-env": "npx ts-node --project scripts/tsconfig.json scripts/set-env.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "21.0.5",
    "@angular/common": "21.0.5",
    "@angular/compiler": "21.0.5",
    "@angular/core": "21.0.5",
    "@angular/forms": "21.0.5",
    "@angular/platform-browser": "21.0.5",
    "@angular/router": "21.0.5",
    "@ngneat/cashew": "5.3.0",
    "@ngrx/signals": "21.0.0",
    "@primeuix/themes": "2.0.2",
    "firebase": "^12.7.0",
    "primeicons": "7.0.0",
    "primeng": "21.0.1",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.16.0"
  },
  "devDependencies": {
    "@angular/build": "21.0.3",
    "@angular/cli": "21.0.3",
    "@angular/compiler-cli": "21.0.5",
    "@angular/localize": "21.0.5",
    "@types/node": "^25.0.3",
    "angular-eslint": "21.1.0",
    "dotenv": "^17.2.3",
    "eslint": "9.39.1",
    "jsdom": "27.3.0",
    "prettier": "3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.47.0",
    "vitest": "4.0.15"
  }
}
